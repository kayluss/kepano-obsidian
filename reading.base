filters:
  and:
    - file.hasLink("Current Fanfiction")
views:
  - type: table
    name: Last Read
    filters:
      and:
        - '!note["Last read"].isEmpty()'
    order:
      - file.name
      - Last read
      - rating
      - topics
      - status
    sort:
      - property: Last read
        direction: DESC
  - type: table
    name: all
